{
  "name": "unitsnet-js",
  "version": "4.0.4",
  "definitionVersion": "6.0.0-pre019",
  "description": "A better way to hold unit variables and easily convert to the destination unit",
  "scripts": {
    "prepublishOnly": "npm run generate && npm run test",
    "build": "tsc",
    "generate": "cd generator-scripts && npm run generate && cd .. && npm run build",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "cover": "node \"node_modules/nyc/bin/nyc.js\" --reporter=lcov --reporter=text-summary npm run test"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/haimkastner/unitsnet-js.git"
  },
  "homepage": "https://github.com/haimkastner/unitsnet-js",
  "author": "Haim Kastner, Based on UnitsNet Project",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.62",
    "@types/numeral": "^2.0.5",
    "axios": "^1.8.4",
    "chai": "^4.2.0",
    "mocha": "^9.2.2",
    "numeral": "^2.0.6",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "xml2js": "^0.6.2"
  },
  "dependencies": {},
  "keywords": [
    "conversion",
    "units-of-measure",
    "units",
    "quantities",
    "unit-converter",
    "converter",
    "unit",
    "measure",
    "measures",
    "measurement",
    "measurements"
  ]
}
